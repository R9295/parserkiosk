    data = {{ t.input.arg }}
    {%- endif %}
    {% if t.assert.func != "fail" -%}
            serialized_data = {{test_func}}(data)
    assert {{ t.assert.func }}(serialized_data, {{ t.assert.arg }})
    {% else -%}
    try:
        serialized_data = {{ test_func }}(data)
        assert False
    except Exception as e:
        {%- if t.assert.get('arg') != None %}
       assert str(e) == '{{t.assert.arg}}'
